<?xml version="1.0"?>

<!--

        DefaultTimeMgr_INPARM_Namelist.xml                              Dec/2005

        Values to use by default for creation of the time-manager model namelists.
        The element names are the same as the corresponding namelist
        variables.  Values that depend on the model configuration use
        attributes to express the dependency.


-->
<timemgr_inparm>

<!-- Coupling time step -->
<atm_cpl_dt>3600</atm_cpl_dt>

<!-- Length of run -->
<stop_option>'ndays'</stop_option>
<stop_n>1</stop_n>

<!-- Length of time for restarts -->
<restart_option>'monthly'</restart_option>

<!-- Starting date.  These match the ic_ymd attributes of the finidat files -->
<!-- in DefaultCLM_INPARM_Namelist.xml.  Note that only dates that are     -->
<!-- different from 101 are specified; 101 is the "default default".       -->
<start_ymd RESOLUTION="1.9x2.5" mask="gx1v4" BGC="none">901</start_ymd>
<start_ymd RESOLUTION="1.9x2.5" mask="gx1v5" BGC="cn"  >25101</start_ymd>
<start_ymd>101</start_ymd>

<!-- Orbit -->
<orb_iyear_ad >2000</orb_iyear_ad>

</timemgr_inparm>
