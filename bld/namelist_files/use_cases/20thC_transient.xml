<?xml version="1.0"?>

<namelist_defaults>

<use_case_desc            >Simulate transient land-use, and aerosol deposition changes from 1850 to 2005</use_case_desc>
<use_case_desc bgc="cn"   >Simulate transient land-use, aerosol deposition, and Nitrogen deposition changes from 1850 to 2005</use_case_desc>
<use_case_desc bgc="cndv" >Simulate transient land-use, aerosol deposition, and Nitrogen deposition changes from 1850 to 2005</use_case_desc>

<start_ymd>18500101</start_ymd>

<sim_year>1850</sim_year>

<sim_year_range>1850-2000</sim_year_range>

<orb_iyear_ad mode="seq_ccsm" >1990</orb_iyear_ad>

<clm_start_type>startup</clm_start_type>

<clm_demand bgc="none"                   >fpftdyn</clm_demand>
<clm_demand bgc="cn"   ndepsrc="data"    >fpftdyn,fndepdyn</clm_demand>
<clm_demand bgc="cndv" ndepsrc="data"    >fpftdyn,fndepdyn</clm_demand>
<clm_demand bgc="cn"   ndepsrc="stream"  >fpftdyn</clm_demand>
<clm_demand bgc="cndv" ndepsrc="stream"  >fpftdyn</clm_demand>
<clm_demand bgc="casa"                   >fpftdyn</clm_demand>

<stream_year_first_ndep bgc="cn"   ndepsrc="stream" >1850</stream_year_first_ndep>
<stream_year_last_ndep  bgc="cn"   ndepsrc="stream" >2005</stream_year_last_ndep>
<model_year_align_ndep  bgc="cn"   ndepsrc="stream" >1850</model_year_align_ndep>

<stream_year_first_ndep bgc="cndv" ndepsrc="stream" >1850</stream_year_first_ndep>
<stream_year_last_ndep  bgc="cndv" ndepsrc="stream" >2005</stream_year_last_ndep>
<model_year_align_ndep  bgc="cndv" ndepsrc="stream" >1850</model_year_align_ndep>

</namelist_defaults>
