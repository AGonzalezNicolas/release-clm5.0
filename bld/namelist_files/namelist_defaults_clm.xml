<?xml version="1.0"?>

<namelist_defaults>

<!--
Values to use by default for creation of CLM model namelists.
The element names are the same as the corresponding namelist
variables.  Values that depend on the model configuration use
attributes to express the dependency.  The recognized attributes
are: RESOLUTION, defaults, mask, ic_ymd, ic_tod, sim_year and all configuration
attributes from the config_cache.xml file (with keys converted to upper-case).
-->

<!-- Default location of {csmdata} -->
<csmdata>/fs/cgd/csm/inputdata</csmdata>

<!-- Default time step -->
<dtime>1800</dtime>

<!-- CO2 volume mixing ratio -->
<co2_ppmv bgc="none" >355.0</co2_ppmv>
<co2_ppmv bgc="casa" >283.1878</co2_ppmv>
<co2_ppmv bgc="cn"   >283.1878</co2_ppmv>
<co2_ppmv bgc="dgvm" >355.0</co2_ppmv>

<!-- ================================================================== -->
<!-- Default resolution -->
<res>1.9x2.5</res>

<!-- Default land/ocean mask type -->
<mask>gx1v5</mask>

<!-- Default simulation year -->
<sim_year>2000</sim_year>

<!-- Default urban air conditioning/heating and wasteheat -->
<urban_hac>OFF</urban_hac>

<!-- Default urban traffic flux -->
<urban_traffic>.false.</urban_traffic>

<!-- ================================================================== -->
<!-- The default filenames are given relative to the root directory
     for the CLM2 data in the CCSM distribution -->

<!-- Plant function types (relative to {csmdata}) -->
<fpftcon           >lnd/clm2/pftdata/pft-physiology.c081002</fpftcon>

