<?xml version="1.0"?>

<!--

       DefaultSettings.xml                             Jul/31/2007

       Values to use by default for creation of CLM model namelists.
       The element names are the same as the corresponding namelist
       variables.

       $Id: DefaultCLM_INPARM_Namelist.xml 5597 2007-07-25 20:45:19Z erik $
-->
<default_settings>

<!-- Root directory of initial and boundary datasets from CCSM distribution -->
<csmdata>/fs/cgd/csm/inputdata</csmdata>

<!-- Default resolution -->
<RESOLUTION >64x128</RESOLUTION>

<!-- Default land/ocean mask type -->
<mask MODE="offline"      >USGS</mask>
<mask MODE="ccsm_seq"     >gx1v4</mask>
<mask MODE="ext_ccsm_con" >gx1v5</mask>

<!-- Default simulation year -->
<sim_year   >2000</sim_year>

<!-- Default begining year for cycling over input atmospheric data -->
<cycle_beg_year MODE="ccsm_seq">2003</cycle_beg_year>
<cycle_beg_year MODE="offline" >1998</cycle_beg_year>

<!-- Default ending year for cycling over input atmospheric data -->
<cycle_end_year MODE="ccsm_seq">2004</cycle_end_year>
<cycle_end_year MODE="offline" >1999</cycle_end_year>

<!-- Model Version -->
<version>
$HeadURL: https://svn-ccsm-models.cgd.ucar.edu/clm2/branches/pftintdat_clm3_5_01/bld/DefaultCLM_INPARM_Namelist.xml $
</version>

</default_settings>
