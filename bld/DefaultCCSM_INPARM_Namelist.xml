<?xml version="1.0"?>

<!--

        DefaultCCSM_INPARM_Namelist.xml                         May/2002

        Values to use by default for creation of the sequential CCSM
        model namelists. The element names are the same as the corresponding namelist
        variables.  Values that depend on the model configuration use
        attributes to express the dependency.


-->
<ccsm_inparm>

<!-- Root directory of initial and boundary datasets from CCSM distribution.
     Filepaths are all relative to this root directory.
-->
<csmdata>/fs/cgd/csm/inputdata</csmdata>

<!-- Case name -->
<case_name>'clmrun'</case_name>

<!-- start-type -->
<start_type>'startup'</start_type>

</ccsm_inparm>

